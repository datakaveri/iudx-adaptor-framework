{
    "name": "jsJob",

    "inputSpec": {
        "type": "http",
        "url": "http://mockserver.adaptor-framework.svc.cluster.local:8080/simpleB",
        "requestType": "GET",
        "pollingInterval": 1000
    },

    "parseSpec": {
        "type": "json",
        "messageContainer": "array",
        "containerPath": "$.data",
        "keyPath": "$.deviceId",
        "timestampPath": "$.time"
    },
    
    "deduplicationSpec": {
        "type": "timeBased"
    },
    
    "transformSpec": {
        "type": "js",
        "script": "function main(obj) { var out = {}; var inp = JSON.parse(obj); out[\"id\"] = \"datakaveri.org/a/b\" + inp[\"id\"]; out[\"k1\"] = inp[\"k1\"]; out[\"observationDateTime\"] = inp[\"time\"]; return JSON.stringify(out); }"
    },

    "publishSpec": {
        "type": "rmq",
        "url": "amqp://rmq-rabbitmq",
	"port": 5672,
        "uname": "guest",
        "password": "guest",
        "sinkName": "adaptor-test",
        "tagName": "test"
    }
}

