{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": [
    "parseSpec",
    "inputData"
  ],
  "properties": {
    "parseSpec": {
      "title": "Parse Spec",
      "type": "object",
      "required": [
        "timestampPath",
        "messageContainer",
        "keyPath",
        "containerPath",
        "inputTimeFormat",
        "outputTimeFormat"
      ],
      "properties": {
        "timestampPath": {
          "title": "Timestamp Path",
          "type": "string"
        },
        "messageContainer": {
          "title": "Message Container",
          "type": "string"
        },
        "keyPath": {
          "title": "Key Path",
          "type": "string"
        },
        "containerPath": {
          "title": "Container Path",
          "type": "string"
        },
        "inputTimeFormat": {
          "title": "Input Time Format",
          "type": "string"
        },
        "outputTimeFormat": {
          "title": "Output Time Format",
          "type": "string"
        }
      }
    }
  ,
    "inputData": {
      "title": "Inputdata",
      "type": "object",
      "required": [
        "outerkey",
        "data"
      ],
      "properties": {
        "outerkey": {
          "title": "Outer Key",
          "type": "string"
        },
        "data": {
          "title": "Data",
          "type": "array",
          "items":{
            "title": "Items",
            "type": "object",
            "required": [
              "time",
              "k1",
              "deviceId"
            ],
            "properties": {
              "time": {
                "title": "Time",
                "type": "string"
              },
              "k1": {
                "title": "K1",
                "type": "string"
              },
              "deviceId": {
                "title": "Device ID",
                "type": "string"
              }
            }
          }
        }
      }
    }
  }
}
